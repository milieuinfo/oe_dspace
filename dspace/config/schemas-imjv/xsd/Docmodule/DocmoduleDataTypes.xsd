			<xs:element name="plichtige" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="intern" type="internType" minOccurs="0"/>
						<xs:element name="verzending" type="verzendingType" minOccurs="0"/>
						<xs:element name="meldingsjaar" type="xs:short" minOccurs="0"/>
						<xs:element name="cbbnummer" type="xs:string" minOccurs="0"/>
						<xs:element name="cbbnummerExploitatie" type="xs:string" minOccurs="0"/>
						<xs:element name="cbbnummerExploitant" type="xs:string" minOccurs="0"/>
						<xs:element name="begeleidend" type="begeleidendType" minOccurs="0"/>
						<xs:element name="identificatie" type="identificatieType" minOccurs="0"/>
						<xs:element name="Energie" type="energieType" minOccurs="0"/>
						<xs:element name="Water" type="WaterType" minOccurs="0"/>
                        <xs:element name="Lucht" type="lucht:Lucht" minOccurs="0"/>
						<xs:element name="afvalproducent" type="afvalproducentType" minOccurs="0"/>
						<xs:element name="afvalverwerker" type="afvalverwerkerType" minOccurs="0"/>
						<xs:element name="grondwater" type="grondwaterType" minOccurs="0" maxOccurs="unbounded"/>
