	</xs:complexType>
	<xs:complexType name="analyseResultatenType">
		<xs:annotation>
			<xs:documentation>deelformulier IV: grondwateranalyseresultaten (blz 4)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="meetwaarde" type="xs:decimal" minOccurs="0"/>
			<xs:element name="detectiecondities" type="xs:string"/>
			<xs:element name="methode" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="grondwateranalyseType">
		<xs:sequence>
			<xs:element name="putnaam" type="xs:string"/>
			<xs:element name="puttype">
				<xs:annotation>
					<xs:documentation>0=peilput;1=pompput;2=installatie, 3=deelinstallatie</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:byte">
						<xs:enumeration value="0"/>
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
						<xs:enumeration value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="monsternummer" type="xs:string"/>
			<xs:element name="monstername_door" type="xs:string"/>
			<xs:element name="datum_monstername" type="datumType" minOccurs="0"/>
			<xs:element name="tijdstip_monstername" type="tijdType" minOccurs="0"/>
			<xs:element name="veldanalyse" minOccurs="0">
				<xs:annotation>
					<xs:documentation>0=Ja;1=Nee</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:byte">
						<xs:enumeration value="0"/>
						<xs:enumeration value="1"/>
					</xs:restriction>
				</xs:simpleType>
