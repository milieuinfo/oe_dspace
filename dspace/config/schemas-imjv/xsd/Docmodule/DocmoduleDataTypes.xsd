			<xs:element name="aanmaningGroep" type="aanmaningMilieuVerslagGroepType" minOccurs="0"/>
			<xs:element name="Lucht" type="lucht:Lucht" minOccurs="0"/>
			<xs:element name="GrondstoffenProducent" type="grondstoffen:GrondstoffenProducent" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
