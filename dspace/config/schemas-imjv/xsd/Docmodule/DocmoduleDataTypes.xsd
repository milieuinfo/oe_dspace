	<xs:complexType name="verzendingType">
		<xs:sequence>
			<xs:element name="versturen_naar">
				<xs:annotation>
					<xs:documentation>
                        0=versturen naar exploitant;
                        1=versturen naar exploitatie;
                        2=versturen naar verzendadres
                    </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:byte">
						<xs:enumeration value="0"/>
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="verzendadres" type="adresType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- bedrijf blokken -->
