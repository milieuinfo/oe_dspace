						<xs:documentation>alle gegevens op het eerste blad van deelformulier IV</xs:documentation>
					</xs:annotation>
					<xs:sequence>
						<!-- id van de vergunning vmm grondwater -->
						<xs:element name="referte" type="referteType" minOccurs="0"/>
						<!-- id van de exploitant vmm grondwater -->
						<xs:element name="id_exploitant_aminal" type="referteType" minOccurs="0"/>
						<xs:element name="watnr" type="xs:string"/>
						<xs:element name="looptijd_van" type="datumType" minOccurs="0"/>
						<xs:element name="looptijd_tot" type="datumType" minOccurs="0"/>
						<xs:element name="klasse" type="xs:string"/>
						<xs:element name="rubriek" type="xs:string"/>
						<xs:element name="grondwaterwinning_te" type="xs:string"/>
						<xs:element name="lokatie" type="xs:string"/>
						<xs:element name="ligging" type="xs:string" minOccurs="0"/>
						<xs:element name="installatienummer" type="xs:string"/>
						<xs:element name="aantal_putten" type="xs:int"/>
						<xs:element name="aquifer_code" type="xs:string"/>
						<xs:element name="vergund_volume_dag" type="xs:decimal" minOccurs="0"/>
						<xs:element name="vergund_volume_jaar" type="xs:decimal" minOccurs="0"/>
						<xs:element name="volume_gewonnen" type="xs:decimal"/>
						<xs:element name="aquifer_omschrijving" type="xs:string"/>
						<xs:element name="opmerkingen" type="xs:string" minOccurs="0"/>
						<xs:element name="putten" minOccurs="0">
							<xs:complexType>
								<xs:annotation>
									<xs:documentation>algemene gegevens van de put</xs:documentation>
								</xs:annotation>
								<xs:sequence>
									<xs:element name="put" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="putnummer" type="xs:string"/>
												<xs:element name="diepte" type="xs:decimal"
												minOccurs="0"/>
												<xs:element name="opgepompte_hoeveelheid"
												type="xs:decimal" minOccurs="0"/>
												<xs:element name="meterstand_vorig_jaar"
												type="xs:decimal" minOccurs="0"/>
												<xs:element name="meterstand_huidig_jaar"
												type="xs:decimal" minOccurs="0"/>
												<xs:element name="datum_ijking" type="datumType"
												minOccurs="0"/>
												<xs:element name="datum_wegname" type="datumType"
												minOccurs="0"/>
												<xs:element name="opmerkingen" type="xs:string"/>
												<!-- toegevoegd -->
												<xs:element name="type" type="putType"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="waterverbruik" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="grondwater" type="waterverbruikGegevensType"
										minOccurs="0"/>
									<xs:element name="hemelwater" type="waterverbruikGegevensType"
										minOccurs="0"/>
									<xs:element name="oppervlaktewater_waterlopen"
										type="waterverbruikGegevensType" minOccurs="0"/>
									<xs:element name="oppervlaktewater_vijvers"
										type="waterverbruikGegevensType" minOccurs="0"/>
									<xs:element name="distributie_drinkwater"
										type="waterverbruikGegevensType" minOccurs="0"/>
									<xs:element name="distributie_industrieel"
										type="waterverbruikGegevensType" minOccurs="0"/>
									<xs:element name="ander" type="waterverbruikGegevensType"
										minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="grondwateranalyse" type="grondwateranalyseType"
							minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="put" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:annotation>
						<xs:documentation>detailgegevens van de put met de peilmetingen (bladzijde 2 van het formulier)
                            en de grondwateranalyse resultaten (bladzijde 3 en 4 van het formulier)
                        </xs:documentation>
					</xs:annotation>
					<xs:sequence>
						<xs:element name="referte" type="referteType" minOccurs="0"/>
						<xs:element name="id_filter_aminal" type="xs:string" minOccurs="0"/>
						<xs:element name="putnummer_exploitant" type="xs:string" minOccurs="0"/>
						<xs:element name="putnaam" type="xs:string"/>
						<xs:element name="puttype" minOccurs="0">
							<xs:annotation>
								<xs:documentation>0=peilput, 1=pompput</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:byte">
									<xs:enumeration value="0"/>
									<xs:enumeration value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="diepte_put" type="xs:decimal" minOccurs="0"/>
						<xs:element name="diepte_onderkant_filter" type="xs:decimal" minOccurs="0"/>
						<xs:element name="lengte_filter" type="xs:decimal" minOccurs="0"/>
						<xs:element name="nivellering" minOccurs="0">
							<xs:annotation>
								<xs:documentation>0=Ja,1=nee</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:byte">
									<xs:enumeration value="0"/>
									<xs:enumeration value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="referentie_meting" minOccurs="0">
							<xs:annotation>
								<xs:documentation>0=maaiveld,1= bovenkant peilbuis, 2=bovenkant beschermbuis, 3=ander
                                    referentiepunt
                                </xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:byte">
									<xs:enumeration value="0"/>
									<xs:enumeration value="1"/>
									<xs:enumeration value="2"/>
									<xs:enumeration value="3"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="referentie_meting_ander" type="xs:string" minOccurs="0"/>
						<xs:element name="methode_peilmeting" minOccurs="0">
							<xs:annotation>
								<xs:documentation>0=peillint,1=elektronische meting,2=ander instrument
                                </xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:byte">
									<xs:enumeration value="0"/>
									<xs:enumeration value="1"/>
									<xs:enumeration value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="methode_peilmeting_ander" type="xs:string" minOccurs="0"/>
						<xs:element name="peilmetingen" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="peilmeting" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="datum" type="datumType"/>
												<xs:element name="tijd" type="tijdType"
												minOccurs="0"/>
												<xs:element name="rust_diepte_grondwater"
												type="xs:decimal" minOccurs="0"/>
												<xs:element name="rust_tijd_pompenstil"
												type="xs:string" minOccurs="0"/>
												<xs:element name="werking_diepte" type="xs:decimal"
												minOccurs="0"/>
												<xs:element name="werking_debiet" type="xs:decimal"
												minOccurs="0"/>
												<xs:element name="tellerstand_debietmeter"
												type="xs:decimal" minOccurs="0"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<!-- complexType geworden -->
						<xs:element name="grondwateranalyse" type="grondwateranalyseType"
							minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="waterverbruikGegevensType">
		<xs:annotation>
			<xs:documentation>deelformulier IV: gegevens betreffende het waterverbruik (blz 1)</xs:documentation>
		</xs:annotation>
		<xs:all minOccurs="0">
			<xs:element name="hoeveelheid" type="xs:decimal" minOccurs="0"/>
			<xs:element name="drinkwater" type="xs:decimal" minOccurs="0"/>
			<xs:element name="proceswater" type="xs:decimal" minOccurs="0"/>
			<xs:element name="koelwater" type="xs:decimal" minOccurs="0"/>
			<xs:element name="spoelwater" type="xs:decimal" minOccurs="0"/>
			<xs:element name="sanitair" type="xs:decimal" minOccurs="0"/>
			<xs:element name="stoomproductie" type="xs:decimal" minOccurs="0"/>
			<xs:element name="landbouw_beregening" type="xs:decimal" minOccurs="0"/>
			<xs:element name="landbouw_hydrocultuur" type="xs:decimal" minOccurs="0"/>
			<xs:element name="landbouw_drinkwatervee" type="xs:decimal" minOccurs="0"/>
