				</xs:annotation>
				<xs:complexType>
					<xs:all minOccurs="0">
						<xs:element name="datum" type="datumType" minOccurs="0"/>
						<xs:element name="geheel">
							<xs:annotation>
								<xs:documentation>1 als volledige stozetting, anders 0</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:byte">
									<xs:enumeration value="0"/>
									<xs:enumeration value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="gedeeltelijk" type="xs:string"/>
						<xs:element name="overgenomen" type="xs:string"/>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="wijziging_overname" minOccurs="0">
				<xs:complexType>
					<xs:all>
						<xs:element name="datum" type="datumType" minOccurs="0"/>
						<xs:element name="geheel">
							<xs:annotation>
								<xs:documentation>1 als volledige overname, anders 0</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:byte">
