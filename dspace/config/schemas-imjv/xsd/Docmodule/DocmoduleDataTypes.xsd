			<xs:element name="referentie" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
                        0=maaiveld;1=bovenkant peilbuis;2=bovenkant beschermbuis;3=ander
                    </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:byte">
						<xs:enumeration value="0"/>
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
						<xs:enumeration value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="referentie_ander" type="xs:string"/>
			<xs:element name="type_pomp" type="xs:string"/>
			<xs:element name="type_slangen" type="xs:string"/>
			<xs:element name="techniek" type="xs:string"/>
			<xs:element name="aanzuring">
				<xs:annotation>
					<xs:documentation>0=Ja; 1=nee</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:byte">
						<xs:enumeration value="0"/>
						<xs:enumeration value="1"/>
					</xs:restriction>
				</xs:simpleType>
