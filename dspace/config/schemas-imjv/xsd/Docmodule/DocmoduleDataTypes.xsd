			<xs:element name="kation_natrium" type="analyseResultatenType" minOccurs="0"/>
			<xs:element name="kation_magnesium" type="analyseResultatenType" minOccurs="0"/>
			<xs:element name="kation_ammonium" type="analyseResultatenType" minOccurs="0"/>
			<xs:element name="kation_mangaan" type="analyseResultatenType" minOccurs="0"/>
			<xs:element name="kation_ijzer_II" type="analyseResultatenType" minOccurs="0"/>
