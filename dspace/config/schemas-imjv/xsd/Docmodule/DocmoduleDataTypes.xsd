				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="datumType">
		<xs:restriction base="xs:token">
			<xs:pattern value="[0-3][0-9][/][0-1][0-9][/][0-2][0-9][0-9][0-9]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tijdType">
		<xs:restriction base="xs:token">
			<xs:pattern value="[0-2][0-9][:][0-5][0-9]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="jaarType">
		<xs:restriction base="xs:token">
			<xs:pattern value="[1-9][0-9][0-9][0-9]"/>
		</xs:restriction>
	</xs:simpleType>
